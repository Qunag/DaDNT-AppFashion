{
  "expo": {
    "name": "FashionApp",
    "slug": "FashionApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/panda.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/panda.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "appfashion",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/panda.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "appfashion"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    }
  }
}